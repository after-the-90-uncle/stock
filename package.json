    "jquery": "^3.2.1",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^3.1.0",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.3.0",
    "koa-sendfile": "^2.0.0",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.3",
