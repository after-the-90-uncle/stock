{
  "name": "admin",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "webpack": "webpack -w",
    "start": "nodemon -w src -x babel-node index",
    "copy": "cp -r static/images dist/static/",
    "dev:server": "NODE_ENV=dev nodemon -w lib -x babel-node tools/dev",
    "dev:webpack": "webpack-dev-server -d",
    "dev:browser-sync": "browser-sync start --proxy localhost:4001 --port 3000 -f static/style",
    "watch": "concurrently  'npm run dev:webpack' 'npm run copy' 'npm run dev:browser-sync' ",
    "dev": "NODE_ENV=dev concurrently 'npm run watch'  'npm run dev:server'",
    "production:server": "NODE_ENV=production nodemon -w lib -x babel-node tools/dev",
    "production": "NODE_ENV=production concurrently  'npm run build' 'npm run production:server' ",
    "build": "concurrently  'webpack -d' 'copyfiles -u 1 static/*.html dist/static/' 'npm run copy'",
    "pm2": "pm2 start --name=\"stock\" npm -- run production",
    "logs": "pm2 logs stock",
    "kill": "pm2 delete stock",
    "status": "pm2 status stock"
  },
  "author": "",
  "license": "ISC",
