    "object-path": "^0.11.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.0.2",
    "redux-thunk": "^2.2.0"
  },
