{
  "presets": ["es2015","react","stage-0", "stage-3"],
  "plugins": [
    "syntax-async-functions",
    "transform-function-bind", 
    ["import", { libraryName: "antd", style: "css" }],
    ["module-alias" , [
    	{"src":"./static/container/common" ,"expose":"common"},
    	{"src":"./static/components" , "expose":"components"},
    	{"src":"./static/constants" , "expose":"constants"},
    	{"src":"./static/services" , "expose":"services"},
      {"src":"./static/utils" , "expose":"utils"},
    	{"src":"./static/redux" , "expose":"selfRedux"},
    ]],
    "react-hot-loader/babel"
  ]
}